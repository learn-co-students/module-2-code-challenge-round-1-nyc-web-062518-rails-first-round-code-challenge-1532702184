<h1>Create a new heroine!</h1>

<% if @heroine.errors %>
  <%= @heroine.errors.full_messages %>
<% end %>

<%= form_for Heroine.new do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %><br>
  <%= f.label :power %>
  <%= collection_select(:power, :id, Power.all, :id, :name, {}) %><br>
  <%= f.submit %>
<% end %>

<%= link_to "return home", heroines_path %>
