<h1>Create new Heroine</h1>

<% if @heroine.errors.any? %>
<% @heroine.errors.full_messages.each do |error| %>
  <%= error %>
  <% end %>
<% end %>

<%= form_for @heroine do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :super_name%>
  <%= f.text_field :super_name %>

  <%= f.collection_select :power_id, Power.all, :id, :name %>

  <%= f.submit %>

<% end %>
