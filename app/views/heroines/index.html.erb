<% if flash[:destroy] %>
  <%= flash[:destroy] %>
<% end %>


<h2 class="center">All Heroines</h2>

<h3 class="right"><%= link_to 'Create A New Super Heroine', new_heroine_path %></h3>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>

